<Window x:Class="CSharpCrawler.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="C#网络爬虫程序" Height="720" Width="1280" WindowStartupLocation="CenterScreen" Style="{StaticResource newStyle}" Closed="Window_Closed">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="30"/>
            <RowDefinition/>
        </Grid.RowDefinitions>

        <WrapPanel Grid.Row="0">
            <Button Width="88" Height="28" Content="抓取Url" Name="btn_FetchUrl" Click="btn_FetchUrl_Click"/>
            <Button Width="88" Height="28" Content="抓取图像" Name="btn_FetchImage" Click="btn_FetchImage_Click"/>
        </WrapPanel>

        <Frame NavigationUIVisibility="Hidden" Name="frame" Grid.Row="1"/>
    </Grid>
</Window>
