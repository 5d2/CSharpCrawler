<?xml version="1.0" encoding="utf-8" ?>
<Crawler>
    <!--公共配置参数-->
    <Common>
	    <!--Url是否合法标志-->
	    <UrlCheck>0</UrlCheck>
	</Common>
	<FetchUrl>
		<Depth>1</Depth>
		<DynamicGrab>0</DynamicGrab>
		<IgnoreUrlCheck>1</IgnoreUrlCheck>
	</FetchUrl>
	<FetchImage>
		<Depth>1</Depth>
		<IgnoreUrlCheck>1</IgnoreUrlCheck>
		<DynamicGrab>1</DynamicGrab>
		<MinSize>0</MinSize>
		<MaxSize>0</MaxSize>
		<MinResolution>0</MinResolution>
		<MaxResolution>0</MaxResolution>
		<!--0 Mix 1-Regex 2-HtmlAgilityPack-->
		<FetchMode>2</FetchMode>
	</FetchImage>
	<!--暂时只做图片-->
	<ThemeList>
		<Theme>
			<Background Type="0">./User Data/Theme/Default.jpg</Background>
		</Theme>
		<Theme>
			<Background Type="0">./User Data/Theme/Default_2.jpg</Background>
		</Theme>
		<Theme>
			<Background Type="0">./User Data/Theme/Default_3.jpg</Background>
		</Theme>
		<Theme>
			<Background Type="0">./User Data/Theme/Default_4.jpg</Background>
		</Theme>
		<Theme>
			<Background Type="1">./User Data/Theme/Default_Dynamic.jpg</Background>
		</Theme>
		<Theme>
			<Background Type="1">./User Data/Theme/Default_Dynamic_2.jpg</Background>
		</Theme>
	</ThemeList>
</Crawler>